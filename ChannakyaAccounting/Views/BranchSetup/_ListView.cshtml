@model Loader.Models.Company
@{
    var test = Model;
    Loader.Service.MenuTemplateService menuService = new Loader.Service.MenuTemplateService();
}




<tr class="detailsList">
    <td class="listDrill" id="@Model.Id" style="vertical-align:top;">
        <a class="inner listDrill" BranchSetup-caption="@Model.BranchName" style="cursor:pointer;"> @Model.BranchName</a>
    </td>
    <td>
     @Model.Address
    </td>
    <td>
       @Model.PhoneNo
    </td>
    <td>
        @Model.Email
        </td>
    <td>
        @Model.Prefix
    </td>

    <td><p data-placement="top" data-toggle="tooltip" title="Edit"><button class="btn btn-primary btn-xs editList" id="@Model.Id" data-title="Edit"><span class="glyphicon glyphicon-pencil"></span></button></p></td>
    <td><p data-placement="top" data-toggle="tooltip" title="Delete"><button class="btn btn-danger btn-xs deleteNode" id="@Model.Id" data-title="Delete"><span class="glyphicon glyphicon-trash"></span></button></p></td>

</tr>



